{
    "manifest_version": 2,
    "name": "Facebook Education Scrapper",
    "description": "Gives us Education Info",
    "version": "1.0",
    "permissions": [
        "tabs",
        "<all_urls>"
    ],
    "content_scripts": [
        {
            "matches": [
                "https://*/*"
            ],
            "js": [
                "Content.js",
                "jquery.min.js"
                
            ]
        }
    ],
    "icons": {
        "16": "images/icon16.png",
        "32": "images/icon32.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
    },
    "browser_action": {
        "default_popup": "popup.html"
    }
}